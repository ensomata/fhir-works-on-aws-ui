  "dependencies": {
    "@awsui/collection-hooks": "^1.0.21",
    "@awsui/components-react": "^3.0.350",
    "@awsui/design-tokens": "^3.0.16",
    "@awsui/global-styles": "^1.0.9",
    "ace-builds": "^1.4.13",
    "axios": "^0.24.0",
    "clone-deep": "^4.0.1",
    "flat": "^5.0.2",
    "mime": "^3.0.0",
    "node-sass": "^6.0.1",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-view": "^1.21.3",
    "react-router-dom": "5",
    "react-scripts": "^4.0.3",
    "store": "^2.0.12",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.2"
  },
  "resolutions": {
    "ws": "^7.4.6",
    "trim-newlines": "^3.0.1",
    "glob-parent": "^5.1.2",
    "normalize-url": "^4.5.1",
    "browserslist": "^4.16.5",
    "tar": "^6.1.2",
    "pac-resolver": "5.0.0",
    "immer": "^9.0.6"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "add-license-header": "license-check-and-add add -f ./license-add-config.json",
    "check-license-header": "license-check-and-add check -f ./license-add-config.json",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/!(@awsui/components-react)"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/clone-deep": "^4.0.1",
    "@types/flat": "^5.0.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/store": "^2.0.2",
    "@types/uuid": "^8.3.1",
    "csp-html-webpack-plugin": "^5.1.0",
    "customize-cra": "^1.0.0",
    "husky": "^7.0.0",
    "license-check-and-add": "^4.0.3",
    "lint-staged": "^12.0.2",
    "prettier": "^2.4.1",
    "react-app-rewired": "^2.1.8",
    "typescript": "^4.4.4"
  }
}
